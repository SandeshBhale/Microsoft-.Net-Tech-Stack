@model DBFirstExampleCURD2.Models.Emp
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Update</title>
</head>
<body>

    @using (Html.BeginForm("Update","Test",FormMethod.Post))
    {
        <label>Emp Name</label><br />
        @Html.EditorFor(p=>p.EmpName)<br />

        <label>Emp Mobile Number</label><br />
        @Html.EditorFor(p => p.Mob)<br />

        @Html.HiddenFor(p=>p.EmpId)

        <label>Departement</label><br />
        @Html.DropDownList("DeptId", "Select Departement");

        <label>Area</label><br />
        @Html.DropDownList("AreaId", "Select Area");
        <br />
        <input type="submit" value="submit"/>
    }

</body>
</html>
