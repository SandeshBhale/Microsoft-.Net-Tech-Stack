@model IEnumerable<CodeFirstExample2.Models.Employee>
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <div> 
        <table border="1">
            <tr>
               <th>@Html.DisplayNameFor(p=>p.EmpId)</th>
                <th>@Html.DisplayNameFor(p=>p.EmpName)</th>
                <th>@Html.DisplayNameFor(p=>p.EmpPost)</th>
                <th>@Html.DisplayNameFor(p=>p.EmpSal)</th>
            </tr>

            @foreach(var temp in Model)
            {
                <tr>
                    <td>@temp.EmpId</td>
                    <td>@temp.EmpName</td>
                    <td>@temp.EmpPost</td>
                    <td>@temp.EmpSal</td>
                </tr>
            }
        </table>
    </div>
</body>
</html>
